{
    "manifest_version": 2,
    "name": "PageMAker",
    "description": "page maker",
    "version": "0",
    "options_page": "options.html",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "background": {
        "page": "bg.html",
        "persistent": false
    },
    "permissions": [
        "tabs",
        "http://*/",
        "https://*/"
    ]
}